{
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types", "./src/types"] ,
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "baseUrl": ".",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/sidebar/page.jsx", "app/reusableComponent/draggable.jsx", "app/reusableComponent/draggable.jsx", "app/api/Listingapis.jsx", "app/reusableComponent/calendar/semimonthlyCalendar.tsx"],
  "exclude": ["node_modules"]

}
